# ternary plot of maxl, maxw, and maxth for Gary points from Poverty Point
# install.packages("ggtern")
library(ggtern)
#set working directory
setwd(getwd())
ppgary<-read.csv("garymorphlm.csv",header = TRUE)
maxl<-ppgary$maxl
maxw<-ppgary$maxw
maxth<-ppgary$maxth
ggtern(data = ppgary,aes(x=maxl,y=maxw,z=maxth)) + geom_point()
maxstl<-ppgary$maxstl
maxstw<-ppgary$maxstw
ggtern(data = ppgary,aes(x=maxl,y=maxw,z=maxstw)) + geom_point()
ggtern(data = ppgary,aes(x=maxl,y=maxth,z=maxstw)) + geom_point()
ggtern(data = ppgary,aes(x=maxl,y=maxw,z=maxstl)) + geom_point()
ggtern(data = ppgary,aes(x=maxl,y=maxth,z=maxstl)) + geom_point()
