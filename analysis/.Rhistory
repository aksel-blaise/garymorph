for (i in 1:(nrow(ppgary))) {
ml <- maxl[i]
mw <- maxw[i]
mt <- maxth[i]
lcheck1 <- ml %in% 80:140
lcheck2 <- ml %in% 46:179
lcheck3 <- ml %in% 33:45
wcheck1 <- mw %in% 30:42
wcheck2 <- mw %in% 20:45
